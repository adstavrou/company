#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/ts-node-dev@2.0.0_@swc+core@1.5.29_@swc+helpers@0.5.17__@types+node@18.16.9_typescript@5.8.3/node_modules/ts-node-dev/lib/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/ts-node-dev@2.0.0_@swc+core@1.5.29_@swc+helpers@0.5.17__@types+node@18.16.9_typescript@5.8.3/node_modules/ts-node-dev/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/ts-node-dev@2.0.0_@swc+core@1.5.29_@swc+helpers@0.5.17__@types+node@18.16.9_typescript@5.8.3/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/ts-node-dev@2.0.0_@swc+core@1.5.29_@swc+helpers@0.5.17__@types+node@18.16.9_typescript@5.8.3/node_modules/ts-node-dev/lib/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/ts-node-dev@2.0.0_@swc+core@1.5.29_@swc+helpers@0.5.17__@types+node@18.16.9_typescript@5.8.3/node_modules/ts-node-dev/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/ts-node-dev@2.0.0_@swc+core@1.5.29_@swc+helpers@0.5.17__@types+node@18.16.9_typescript@5.8.3/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin.js" "$@"
else
  exec node  "$basedir/../../lib/bin.js" "$@"
fi
