#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/nx@20.8.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.21_ty_24bzhxgp2bevvmfbj23qmqh36u/node_modules/nx/bin/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/nx@20.8.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.21_ty_24bzhxgp2bevvmfbj23qmqh36u/node_modules/nx/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/nx@20.8.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.21_ty_24bzhxgp2bevvmfbj23qmqh36u/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/nx@20.8.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.21_ty_24bzhxgp2bevvmfbj23qmqh36u/node_modules/nx/bin/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/nx@20.8.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.21_ty_24bzhxgp2bevvmfbj23qmqh36u/node_modules/nx/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/nx@20.8.1_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+types@0.1.21_ty_24bzhxgp2bevvmfbj23qmqh36u/node_modules:/Users/alexandrosstavrou/Projects/mine/company/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nx/bin/nx-cloud.js" "$@"
else
  exec node  "$basedir/../nx/bin/nx-cloud.js" "$@"
fi
