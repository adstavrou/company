{
  "generators": {
    "barrel": {
      "factory": "./tools/src/generators/barrel",
      "schema": "./tools/src/generators/barrel/schema.json",
      "description": "Generate barrel index.ts file for a library"
    },
    "custom-app": {
      "factory": "./src/generators/custom-app/index.ts",
      "schema": "./src/generators/custom-app/schema.json",
      "description": "Create a new custom app (with build ready)"
    },
    "custom-lib": {
      "factory": "./tools/src/generators/custom-lib",
      "schema": "./tools/src/generators/custom-lib/schema.json",
      "description": "Create a custom library"
    },
    "custom-service": {
      "factory": "./tools/src/generators/custom-service",
      "schema": "./tools/src/generators/custom-service/schema.json",
      "description": "Create a new service class inside a library"
    },
    "custom-utility": {
      "factory": "./tools/src/generators/custom-utility",
      "schema": "./tools/src/generators/custom-utility/schema.json",
      "description": "Generates a utility function inside an existing library"
    },
    "add-demo-code": {
      "factory": "./tools/src/generators/add-demo-code/index.ts",
      "schema": "./tools/src/generators/add-demo-code/schema.json",
      "description": "Add a new demo snippet to demo-code-snippets app"
    }
  }
}