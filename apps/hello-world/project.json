{
    "name": "hello-world",
    "projectType": "application",
    "root": "apps/hello-world",
    "sourceRoot": "apps/hello-world/src",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "tsc -p apps/hello-world/tsconfig.app.json"
        }
      },
      "serve": {
        "executor": "nx:run-commands",
        "options": {
            "command": "ts-node-dev --respawn apps/hello-world/src/main.ts"
            }
        }
    }
}
